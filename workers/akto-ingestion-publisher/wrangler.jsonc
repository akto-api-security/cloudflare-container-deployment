{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "akto-ingestion-publisher",
	"main": "src/index.ts",
	"compatibility_date": "2025-06-23",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"observability": {
		"enabled": true
	},
	"services": [
		{
			"binding": "AKTO_INGESTION_WORKER",
			"service": "akto-ingest-guardrails"
		}
	]

	/**
	 * Note: Secrets should be set via CLI:
	 * wrangler secret put AKTO_API_KEY
	 */
}
